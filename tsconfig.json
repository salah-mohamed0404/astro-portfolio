{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@utils/*": ["src/utils/*"],
			"@constants/*": ["src/constants/*"],
			"@content/*": ["src/content/*"],
			"@assets/*": ["src/assets/*"],
			"@translations/*": ["public/locales/*"]
		}
	}
}
